@charset "utf-8";

@import url('../bootstrap.css');
@import url('./bootstrap-theme.css');

body {
  max-width: 190mm;
  margin: 0 auto;
}

.row {
  border: 1px dotted #8be;
  padding: 3px 0;
}

div[class*="col"] {
  border: 1px dotted #9d6;
  padding: 3px 0;
}
</mt:Unless>